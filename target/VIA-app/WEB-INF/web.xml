<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GenrateGraph</servlet-name>
  	<display-name>GenrateGraph</display-name>
  	<description></description>
  	<servlet-class>testcheck.GenrateGraph</servlet-class>
  </servlet>
  <servlet>
   <servlet-name>contact</servlet-name>
   <jsp-file>/contact.jsp</jsp-file>
</servlet>
<servlet>
   <servlet-name>page2dynamic</servlet-name>
   <jsp-file>/page2dynamic.jsp</jsp-file>
</servlet>
  <servlet-mapping>
  	<servlet-name>contact</servlet-name>
  	<url-pattern>/contact</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>page2dynamic</servlet-name>
    <url-pattern>/page2dynamic</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GenrateGraph</servlet-name>
    <url-pattern>/genrategraph</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
</welcome-file-list>
</web-app>
